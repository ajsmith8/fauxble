<div id = 'home' class = "header_logo_container">
	<div class = "image_container">
		<div class = "blue_logo"><img src = 'http://s3.amazonaws.com/fusegap/logos/headerlogo.png' width = "100%" /></div>
		<div class = "white_logo"><img src = 'http://s3.amazonaws.com/fusegap/logos/headerlogo_hover.png' width = "100%" /></div>
	</div>
</div>
<div class = 'signin panel hide'>
	<div class = "facebook_login_button_container">
