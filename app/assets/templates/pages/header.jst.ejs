			<div class = "left">
				<div class = "logo"><img src = 'http://s3.amazonaws.com/fusegap/logos/white_25.png' width = "100%" /></div>
			</div>
			<div id = 'get_started' class = "right">Get Started</div>
		</div>
	</div>
</div>
<div class = "centered_header_container">
	<div id = 'profile' class = "profile_header"></div>
</div>
<% if (user) { %>
	<div class = "triangle_container">
		<div class = "triangle"></div>
	</div>
	<div id = 'signout' class = 'hide'>Sign Out?</div>
<% } %>
































